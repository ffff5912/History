{
    "manifest_version": 2,
    "name": "History",
    "version": "0.0.1",
    "description": "Access to browser history",
    "author": "ffff5912",
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "history",
        "chrome://favicon/"
    ],
    "browser_action": {
        "default_title": "History",
        "default_popup": "popup.html",
        "default_icon": {
        }
    }
}
